button,
input,
optgroup,
option,
select,
textarea {
    font-family: inherit;
    font-size: inherit;
    font-style: inherit;
    font-weight: inherit;
    text-align: center;
    outline: 0;
}
select popup{
    color:#000;
    background: #fff;
    overflow-y: scroll-indicator;
    overflow-x: hidden;
}
select popup.list{
    background: #fff;
    padding: 0px;
    max-height: 130px;
    max-height: 50px;
}
.layui-form-radio {
    line-height: 28px;
    margin: 6px 10px 0 0;
    padding-right: 10px;
    cursor: pointer;
    font-size: 0;
}

.layui-form-radio,
.layui-form-radio * {
    display: inline-block;
    vertical-align: middle;
}

.layui-form-select .layui-input {
    padding-right: 30px;
    cursor: pointer;
}

.layui-form-item .layui-form-checkbox {
    margin-top: 4px;
}

.layui-form-checked,
.layui-form-checked:hover {
    border-color: #5FB878;
}

.layui-form-checkbox {
    position: relative;
    height: 30px;
    line-height: 30px;
    margin-right: 10px;
    padding-right: 30px;
    cursor: pointer;
    font-size: 0;
    -webkit-transition: .1s linear;
    transition: .1s linear;
    box-sizing: border-box;
}

.layui-form-checkbox,
.layui-form-checkbox *,
.layui-form-switch {
    display: inline-block;
    vertical-align: middle;
}

.layui-form-checked i,
.layui-form-checked:hover i {
    color: #5FB878;
}

.layui-form-checkbox i {
    position: absolute;
    right: 0;
    top: 0;
    width: 30px;
